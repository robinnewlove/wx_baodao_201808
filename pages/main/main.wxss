page{
  background-color: #eeeeee;
}
.page{background: #eeeeee;}


.top{height: 128rpx; text-align: center; font-size:30rpx; background: #eeeeee;color: #84868a;line-height:180%;padding-top: 14rpx;font-weight: normal; overflow: hidden}
.banner{background: url('https://werun.renlai.fun/static/images/banner.jpg');height: 280rpx; background-size: 100% auto; color: #fff; text-align: center;font-size: 34rpx;padding-top: 20rpx}

.userinfobox{padding:38rpx 30rpx 0 38rpx;overflow: hidden;background: #fff;}
.userheader{width:56rpx; height: 56rpx;border-radius:56rpx; float: left; overflow: hidden; margin-right: 20rpx;}
.userheader image{width:56rpx; height: 56rpx;}
.userheadertxt{float: left; height: 56rpx; line-height: 56rpx;font-size: 44rpx;color:#666;}

.navigator-guize{float: right; height: 56rpx; line-height: 56rpx;color:#1c1c1c;font-size: 26rpx;}

.allstep{color: #c9ff27; font-size: 70rpx;padding: 0 5rpx; position: relative;top:3rpx;}

.navigator-exchange{background: url('https://werun.renlai.fun/static/images/btn-1.png');width: 194rpx;  height: 68rpx; background-size: 100% auto; margin: auto; margin-top: 20rpx;}

.navigator-share{background: url('https://werun.renlai.fun/static/images/btn-2.png');width: 194rpx;  height: 68rpx; background-size: 100% auto; margin: auto; margin-top: 20rpx;}

/*圆形进度条*/
.circleBar{width: 100%;text-align: center; background: #fff;padding: 15rpx 0 42rpx 0; margin-bottom:20rpx;}
.circleBarbody{display: inline-block;position: relative; text-align: center;}
.centerWord{position: absolute;top:0;font-size: 24rpx;text-align: center;width: 100%;color: #a7a7a7; padding-top: 80rpx}
.centerWord .step{display: block;font-size: 50rpx;color: #333;}

/*分享*/
.sharebox{position:fixed; bottom: 0px;width: 100%; height: 250rpx;background: #fff; border-top: 1px solid #eee;bottom:-450rpx;z-index: 99;}
.sharebox text{display: block; text-align: center;font-size: 24rpx; color: #666;}
.sharebox-1{width: 50%; height: 150rpx;float: left;text-align: center; padding-top: 50rpx;}
.share-1{width:120rpx; height: 100rpx;background: url('https://werun.renlai.fun/static/images/share-ico-1.png');background-size: 120rpx 100rpx; border: 0px;}
.share-1::after{
  border: none;
}

.share-2{width:120rpx; height: 100rpx;background: url('https://werun.renlai.fun/static/images/share-ico-2.png');background-size: 120rpx 100rpx; border: 0px;}
.share-2::after{
  border: none;
}

.sharebox-2{width: 50%; height: 150rpx;float: left;text-align: center; padding-top: 50rpx;}
.sharebox-2 image{width:120rpx; height: 100rpx;}

/*遮照*/
.maskbox{position:fixed;background: #000;width:100%;top:0;bottom: 0;opacity: 0.2;z-index: 98;display: none}
.show{display: inline-block;}

/*朋友数据列表*/

.userstep-list{background:#fff;padding-top: 36rpx;overflow: hidden;margin-bottom: 20rpx;}
.userstep-list-nodata{margin-bottom: 40rpx;padding-left: 30rpx; font-size: 26rpx;}
.userstep-col{overflow: hidden;padding-left: 57rpx;padding-right: 57rpx; margin-bottom: 26rpx;}
.userstep-header{width: 36rpx; height: 36rpx;border-radius: 36rpx; float: left; margin-right: 17rpx;}
.userstep-name{font-size: 24rpx;height: 36rpx; line-height: 36rpx; float: left; }
.userstep-num{float: right;height: 36rpx; line-height: 36rpx; color: #848484; font-size: 24rpx;}

.userOrderList{background: #fff;}
.userOrderList-title{height: 100rpx; line-height: 100rpx;border-bottom: 1px solid #e5e5e5;padding-left: 60rpx;}
.userOrderListbody{padding-top: 36rpx;overflow: hidden;}
.userOrderList-col{overflow: hidden;padding-left: 77rpx;padding-right: 57rpx; margin-bottom: 26rpx;position:relative}
.userOrderList-header{width: 56rpx; height: 56rpx;border-radius: 56rpx; float: left; margin-right: 17rpx;}
.userOrderList-progress{width: 420rpx;float: left;}

.userOrderList-name{font-size: 24rpx;height: 36rpx; line-height: 36rpx; float: left;width: 420rpx; color: #5c5c5c; }
.userOrderList-num{float: right;height: 36rpx; line-height: 36rpx; color: #848484; font-size: 24rpx;}

.shareimgbox{position: fixed;top:0px;bottom: 0px;background: #000000;width: 100%;padding-top: 50px; overflow: hidden}

.shareimgboxbody{margin: auto;margin-bottom: 20px;position: relative;}
.closeshare{position: absolute;width: 60rpx;height: 60rpx; display: inline-block;top:-70rpx;right: -70rpx;background: url('https://werun.renlai.fun/static/images/close.png'); background-size: 100% 100%;padding: 0px; border: 0px;z-index: 9999}
.closeshare::after{
  border: none;
}

.btn-save-local{height: 80rpx;line-height: 80rpx;font-size: 30rpx;}

.share-image{position: absolute}

.canvasbox{width: 0px; height: 0px;overflow: hidden}

